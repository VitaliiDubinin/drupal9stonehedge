# Compose project name
COMPOSE_PROJECT_NAME=stonehenge

# Root domain for all local services
DOCKER_DOMAIN=docker.so

# Logo for Portainer
LOGO_URL=https://s3.eu-central-1.amazonaws.com/druid-edge/stonehenge_logo_with_text.png

# Prefix for containers
PREFIX=stonehenge

# Stonehenge major version
STONEHENGE_VERSION=3

# Image versions
TRAEFIK_VERSION=2.5.3
PORTAINER_VERSION=2.9.1
SSH_IMAGE_VERSION=alpine-3.14.2
